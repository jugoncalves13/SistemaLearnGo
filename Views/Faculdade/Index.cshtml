@model IEnumerable<SistemaLearnGo.Models.Faculdade>

@{
    ViewData["Title"] = "Index";
}

<h1>Faculdade</h1>

<p>
    <a asp-action="Create">Criar novo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FaculdadeNome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FaculdadeCidade)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FaculdadeNome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FaculdadeCidade)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FaculdadeId">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.FaculdadeId">Detalhes</a> |
                <a asp-action="Delete" asp-route-id="@item.FaculdadeId">Deletar</a>
            </td>
        </tr>
}
    </tbody>
</table>
